    <Report type="Table">
        <Table>SettlersOfCatan_GamePlayer</Table>
        <Name>Settlers Players</Name>
        <Title>Settlers Players</Title>
        <Filter />
		<ResultFilter>
			<Expression type="GreaterThanOrEqual">
				<Left>
					<Expression type="Column">
						<Name>GamesPlayed</Name>
					</Expression>
				</Left>
				<Right>
					<Expression type="Value">
						<Type>Integer</Type>
						<Value>10</Value>
					</Expression>
				</Right>
			</Expression>
		</ResultFilter>
        <Group>true</Group>
        <Columns>
            <Column>
                <Header>Player</Header>
                <Width>200</Width>
                <Name>Player</Name>
                <GroupRule>By</GroupRule>
                <SortRule>Descending</SortRule>
                <ColumnExpression>
                    <Expression type="Field">
                        <FieldPath>GamePlayer.Player.Name</FieldPath>
                    </Expression>
                </ColumnExpression>
                <DataType>String</DataType>
            </Column>
			<Column>
				<Header>Played</Header>
				<Width>200</Width>
				<Name>GamesPlayed</Name>
				<GroupRule>Sum</GroupRule>
				<SortRule>Ascending</SortRule>
				<ColumnExpression>
					<Expression type="Value">
						<Type>Integer</Type>
						<Value>1</Value>
					</Expression>
				</ColumnExpression>
				<DataType>Integer</DataType>
			</Column>
            <Column>
                <Header>Wins</Header>
                <Width>200</Width>
                <Name>Wins</Name>
                <GroupRule>Sum</GroupRule>
                <SortRule>Descending</SortRule>
                <ColumnExpression>
                    <Expression type="If">
                        <Condition>
                            <Expression type="Field">
                                <FieldPath>Winner</FieldPath>
                            </Expression>
                        </Condition>
                        <TrueValue>
                            <Expression type="Value">
                                <Type>Integer</Type>
                                <Value>1</Value>
                            </Expression>
                        </TrueValue>
                        <FalseValue>
                            <Expression type="Value">
                                <Type>Integer</Type>
                                <Value>0</Value>
                            </Expression>
                        </FalseValue>
                    </Expression>
                </ColumnExpression>
                <DataType>Integer</DataType>
            </Column>
			<Column>
				<Header>Win Average</Header>
                <Width>200</Width>
                <Name>WinAverage</Name>
                <GroupRule>Average</GroupRule>
                <SortRule>Descending</SortRule>
                <ColumnExpression>
                    <Expression type="If">
                        <Condition>
							<Expression type="Field">
								<FieldPath>Winner</FieldPath>
							</Expression>
						</Condition>
						<TrueValue>
							<Expression type="Value">
								<Type>Number</Type>
								<Value>1.0</Value>
							</Expression>
						</TrueValue>
						<FalseValue>
							<Expression type="Value">
								<Type>Number</Type>
								<Value>0.0</Value>
							</Expression>
						</FalseValue>
                    </Expression>
                </ColumnExpression>
                <DataType>Number</DataType>
			</Column>
            <Column>
                <Header>Best Score</Header>
                <Width>200</Width>
                <Name>BestScore</Name>
                <GroupRule>Max</GroupRule>
                <SortRule>Descending</SortRule>
                <ColumnExpression>
                    <Expression type="Field">
                        <FieldPath>Points</FieldPath>
                    </Expression>
                </ColumnExpression>
                <DataType>Integer</DataType>
            </Column>
            <Column>
                <Header>Average Score</Header>
                <Width>200</Width>
                <Name>AverageScore</Name>
                <GroupRule>Average</GroupRule>
                <SortRule>Descending</SortRule>
                <ColumnExpression>
                    <Expression type="Field">
                        <FieldPath>Points</FieldPath>
                    </Expression>
                </ColumnExpression>
                <DataType>Integer</DataType>
            </Column>
            <Column>
                <Header>Worst Score</Header>
                <Width>200</Width>
                <Name>WorstScore</Name>
                <GroupRule>Min</GroupRule>
                <SortRule>Descending</SortRule>
                <ColumnExpression>
                    <Expression type="Field">
                        <FieldPath>Points</FieldPath>
                    </Expression>
                </ColumnExpression>
                <DataType>Integer</DataType>
            </Column>
        </Columns>
    </Report>
