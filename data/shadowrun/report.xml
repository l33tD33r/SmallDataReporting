<?xml version="1.0" encoding="UTF-8"?>
<Reports>
    <Report type="Table">
        <Table>Shadowrun_ArmorPiercing</Table>
        <Name>WeaponAp</Name>
        <Title>Weapon AP</Title>
        <SourceFilter>
            <Expression type="LessThanOrEqual">
                <Left>
                    <Expression type="Field">
                        <FieldPath>ArmorPiercing</FieldPath>
                    </Expression>
                </Left>
                <Right>
                    <Expression type="Value">
                        <Type>Number</Type>
                        <Value>6</Value>
                    </Expression>
                </Right>
            </Expression>
        </SourceFilter>
        <ResultFilter>
            <Expression type="Not">
                <Child>
                    <Expression type="Equals">
                        <Left>
                            <Expression type="Column">
                                <Name>Cartridge</Name>
                            </Expression>
                        </Left>
                        <Right>
                            <Expression type="Value">
                                <Type>String</Type>
                                <Value>Buckshot</Value>
                            </Expression>
                        </Right>
                    </Expression>
                </Child>
            </Expression>
        </ResultFilter>
        <Chart>
            <Type>areaspline</Type>
            <Title>Weapon AP</Title>
            <SeriesNameColumn>Cartridge</SeriesNameColumn>
            <SeriesDataColumns>
                <SeriesDataColumn>Distance</SeriesDataColumn>
                <SeriesDataColumn>AP</SeriesDataColumn>
            </SeriesDataColumns>
        </Chart>
        <Columns>
            <Column>
                <Header>Cartridge</Header>
                <Width>300</Width>
                <Name>Cartridge</Name>
                <ColumnExpression>
                    <Expression type="Field">
                        <FieldPath>Cartridge.Name</FieldPath>
                    </Expression>
                </ColumnExpression>
                <DataType>String</DataType>
            </Column>
            <Column>
                <Header>Muzzle Velocity</Header>
                <Width>300</Width>
                <Name>MV</Name>
                <ColumnExpression>
                    <Expression type="Field">
                        <FieldPath>MuzzleVelocity</FieldPath>
                    </Expression>
                </ColumnExpression>
                <DataType>Number</DataType>
            </Column>
            <Column>
                <Header>Key</Header>
                <Width>200</Width>
                <Name>Key</Name>
                <ColumnExpression>

                </ColumnExpression>
            </Column>
            <Column>
                <Header>Distance</Header>
                <Width>100</Width>
                <Name>Distance</Name>
                <ColumnExpression>
                    <Expression type="Field">
                        <FieldPath>Distance</FieldPath>
                    </Expression>
                </ColumnExpression>
                <DataType>Number</DataType>
            </Column>
            <Column>
                <Header>Armor Piercing</Header>
                <Width>100</Width>
                <Name>AP</Name>
                <ColumnExpression>
                    <Expression type="Field">
                        <FieldPath>ArmorPiercing</FieldPath>
                    </Expression>
                </ColumnExpression>
                <DataType>Number</DataType>
            </Column>
        </Columns>
    </Report>
